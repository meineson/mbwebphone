{
  "name": "mbwebphone",
  "version": "1.0.2",
  "main": "app.html",
  "node-main": "miniweb.js",
  "node-remote": "http://localhost:3000",
  "chromium-args": "--ignore-certificate-errors",
  "no-edit-menu": true,
  "type": "commonjs",
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.15.0"
  },
  "window": {
    "icon": "logo.icns",
    "width": 1066,
    "height": 600,
    "resizable": true
  },
  "scripts": {
    "dev": "/Users/mac/nwjs/nwjs.app/Contents/MacOS/nwjs .",
    "dist": "tar zcvf ./out/mbwebphone.tgz index.html jssip-3.10.0.min.js mbphone.js mbweb.css miniweb.js && build --tasks mac-x64 --concurrent .",    
    "start": "run --x64 .",
    "dmg": "hdiutil create -volname 'MBWebPhone' -srcfolder './out/MBWebPhone APP 1.0.1 mac x64/MBWebPhone.app' -ov -format UDZO './out/MBWebPhone.dmg'"
  },
  "author": "meineson",
  "license": "MIT",
  "build": {
    "appId": "mbwebphone.cn.mbstudio",
    "nwVersion": "0.101.0",
    "ffmpegIntegration": true,
    "packed": true,
    "output": "./out",
    "files": [
      "**/*"
    ],
    "targets": [
      "zip",
      "nsis7z"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "MBWebPhone",
      "companyName": "MBSTUDIO",
      "copyright": "copyright (C) mbstudio.cn"      
    },
    "mac": {
      "displayName": "MBWebPhone",
      "copyright": "copyright (C) mbstudio.cn",
      "icon": "logo.icns",
      "plistStrings": {
        "CFBundleIdentifier": "mbwebphone.cn.mbstudio",
        "CFBundleDocumentTypes": []
      }
    }
  }
}
